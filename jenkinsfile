pipeline {
    agent any
    stages {
        stage('Stage Example') {
             git(
                url: 'git@github.com:thenerdics/mbJenkins.git',
                branch: "piepelineStages"
            )
        }
        stage('Steps Example'){
            steps {
                sh 'echo "Hello world"'
            }
        }
    }
    post { 
        always { 
            echo 'I will always say Hello again!'
        }
    }
}